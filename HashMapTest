print("\n\n\nTesting 1 dictionarty  creation-----------------------------------------------------------")
tab = {"11111112":'011111112', "11111121":'011111121', "11111211":'011111211', "11112111":'011112111',
       "11121111":'011121111', "11211111":'011211111', "12111111":'012111111', "21111111":'021111111'}
print("Table size is: " + str(len(tab)) )
print(tab)

print("\n\n\nTesting 2 get-----------------------------------------------------------")
print(tab['11111112'])

print("\n\n\nTesting 3 modify-----------------------------------------------------------")
tab['11111112']='aaaaaaaaaa'
print(tab['11111112'])

print("\n\n\nTesting 4 pop-----------------------------------------------------------")
tab.pop('11111112')
print(tab)